<!DOCTYPE html>

<html>
<head>
    <title>@ViewBag.Title</title>
    <meta name="keywords" content="Вибрафон, вибрафонист, джаз, Иннокентий Иванов, Ivanov Vibe Band, джаз-квинтет, этно-джаз, фьюжн, москва, Антон Залетаев, Вячеслав Ахметзянов, Алексей Денисов, Сергей Рощупкин" />
    <meta name="description" content="Ivanov Vibe Band московский джаз-проект под руководством вибрафониста и композитора Иннокентия Иванова. Квинтет исполняет авторскую музыку в стиле этно-джаз, фьюжн, латин-джаз, би-боп.">
    <link rel="SHORTCUT ICON" href="@Url.Content("~/content/images/favicon.ico")"/>
    <link href="@Url.Content("~/Content/Css/style.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Css/font-awesome.min.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-2.1.3.min.js")" type="text/javascript"></script>
    @if (IsSectionDefined("HeadSection"))
    {
        @RenderSection("HeadSection")
    }
</head>
<body>
    <div id="fb-root"></div>
    <script>(function (d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) return;
        js = d.createElement(s); js.id = id;
        js.src = "//connect.facebook.net/ru_RU/all.js#xfbml=1&appId=495454623820128";
        fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));</script>
    <div class="layout">
        <div class="header">
            @if (IsSectionDefined("SliderSection"))
            {
                @RenderSection("SliderSection")
            }
            <div class="nav">
                <ul>
                    <li class="@Html.ActiveTab("Home", new string[]{"Index"}, "active")">@Html.ActionLink("Главная", "Index", "Home")</li>
                    <li class="@Html.ActiveTab("Article", new string[]{"List", "Details"}, "active")">@Html.ActionLink("Новости", "List", "Article")</li>
                    <li class="@Html.ActiveTab("Song", new string[]{"Index"}, "active")">@Html.ActionLink("Музыка", "Index", "Song")</li>
                    <li class="@Html.ActiveTab("Video", new string[]{"List", "Details"}, "active")">@Html.ActionLink("Видео", "List", "Video")</li>
                    <li class="@Html.ActiveTab("About", new string[]{"List", "Details"}, "active")">@Html.ActionLink("О группе", "List", "About")</li>
                    <li class="@Html.ActiveTab("Contact", new string[]{"Index"}, "active") last">@Html.ActionLink("Контакты", "Index", "Contact")</li>
                </ul>
            </div>
        </div>
        <div class="content">
            <div class="container">
                <div class="left-col">
                   @if (IsSectionDefined("LeftSection"))
                   {
                       @RenderSection("LeftSection")
                   }
                </div>
                <div class="right-col">
                    @if (IsSectionDefined("RightSection"))
                    {
                       @RenderSection("RightSection")
                    }
                </div>
                <div class="clear"></div>
            </div>
        </div>
        <div class="footer">
            <div class="container">
                <div class="reserved">© @DateTime.Now.Year. Ivanovband. Все права защищены.</div>
                <div class="support">Техническая поддержка: <a href="mailto:support@ivanovband.com">support@ivanovband.com</a></div>
                <div class="follow-buttons">@Html.Partial("_FollowButtons")</div>
                <div class="clear"></div>
            </div>
        </div>
    </div>
    <script type="text/javascript">
        /* * * CONFIGURATION VARIABLES: EDIT BEFORE PASTING INTO YOUR WEBPAGE * * */
        var disqus_shortname = 'ivanovband';

        /* * * DON'T EDIT BELOW THIS LINE * * */
        (function () {
            var s = document.createElement('script'); s.async = true;
            s.type = 'text/javascript';
            s.src = '//' + disqus_shortname + '.disqus.com/count.js';
            (document.getElementsByTagName('HEAD')[0] || document.getElementsByTagName('BODY')[0]).appendChild(s);
        }());
    </script>
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');
        ga('create', 'UA-39559469-1', 'ivanovband.com');
        ga('send', 'pageview');
    </script>
    <!-- Yandex.Metrika counter -->
    <script type="text/javascript">
        (function (d, w, c) {
            (w[c] = w[c] || []).push(function () {
                try {
                    w.yaCounter20716600 = new Ya.Metrika({
                        id: 20716600,
                        clickmap: true,
                        trackLinks: true,
                        accurateTrackBounce: true
                    });
                } catch (e) { }
            });

            var n = d.getElementsByTagName("script")[0],
                s = d.createElement("script"),
                f = function () { n.parentNode.insertBefore(s, n); };
            s.type = "text/javascript";
            s.async = true;
            s.src = (d.location.protocol == "https:" ? "https:" : "http:") + "//mc.yandex.ru/metrika/watch.js";

            if (w.opera == "[object Opera]") {
                d.addEventListener("DOMContentLoaded", f, false);
            } else { f(); }
        })(document, window, "yandex_metrika_callbacks");
    </script>
    <noscript><div><img src="//mc.yandex.ru/watch/20716600" style="position:absolute; left:-9999px;" alt="" /></div></noscript>
    <!-- /Yandex.Metrika counter -->
</body>
</html>
